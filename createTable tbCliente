use guilherme_test;

CREATE TABLE tbcliente (
    cpf VARCHAR(11) PRIMARY KEY,
    nome VARCHAR(100),
    endereco1 VARCHAR(255),
    endereco2 VARCHAR(255),
    bairro VARCHAR(100),
    cidade VARCHAR(100),
    UF VARCHAR(2),
    cep VARCHAR(8),
    idade INT,
    sexo ENUM('M', 'F'),
    limite_credito DECIMAL(10,2),
    volume_compra DECIMAL(10,2),
    primeira_compra DATE,
    data_nascimento DATE
);
